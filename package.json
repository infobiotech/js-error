    "@babel/preset-env": "7.15.6",
    "@babel/preset-flow": "7.14.5",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.2.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "4.0.0",
    "esm": "^3.2.25",
    "jest": "27.2.0",
    "mocha": "^9.1.1",
    "nodemon": "2.0.12",
    "prettier": "2.4.1",
    "rimraf": "3.0.2"

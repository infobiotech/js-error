{
  "name": "@infobiotech/js-error",
  "author": "Alessandro Raffa <a.raffa@infobiotech.com> (https://infobiotech.com)",
  "version": "0.1.7",
  "keywords": [
    "error"
  ],
  "license": "MIT",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infobiotech/js-error.git"
  },
  "bugs": {
    "url": "https://github.com/infobiotech/js-error/issues"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "nodemonConfig": {
    "exec": "yarn build",
    "watch": [
      "src/*"
